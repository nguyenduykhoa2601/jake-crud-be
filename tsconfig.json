{
	"compilerOptions": {
		"target": "ES2017",
		"module": "commonjs",
		"outDir": "./dist",
		"rootDir": "./",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true
	},
	"include": [
		"app.ts",
		"controllers/**/*.ts",
		"routes/**/*.ts",
		"interfaces/**/*.ts",
		"models/**/*.ts"
	]
}
